<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:localy="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <localy:Categorys x:Key="Categorys"/>
        <localy:EnumToStringConverter x:Key ="EnumToStringConverter"/>
        <localy:ZeroToHidden x:Key="ZeroToHidden"/>
        <localy:ZeroToVisible x:Key="ZeroToVisible"/>
        <localy:ZeroToFalse x:Key="ZeroToFalse"/>
        <localy:OneToFalse x:Key="OneToFalse"/>
        <localy:ZeroIntToEmptyString x:Key="ZeroIntToEmptyString"/>
        <localy:ZeroDoubleToEmptyString x:Key="ZeroDoubleToEmptyString"/>
        <localy:AllToHidden x:Key="AllToHidden"/>
        <localy:TextLengthToTrue x:Key="TextLengthToTrue"/>
        <localy:NullToHidden x:Key="NullToHidden"/>
        <localy:NullToVisible x:Key="NullToVisible"/>
        <localy:arrayLengthToVisibility x:Key="arrayLengthToVisibility"/>
        <localy:NullToFalse x:Key="NullToFalse"/>
        <localy:MaxAmountToTrue x:Key="MaxAmountToTrue"/>
        <localy:MinAmountToTrue x:Key="MinAmountToTrue"/>
        <localy:textBoxFullToTrue x:Key="textBoxFullToTrue"/>
        <localy:convertTextBoxFullToTrue x:Key="convertTextBoxFullToTrue"/>

    </Application.Resources>
</Application>
